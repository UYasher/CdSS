/* The warm-up exercises from hw2, converted to cdss */
#square1 {
    height: v(x)px;
    width: v(x)px;
    z-index: v(r);
}

#square2 {
    height: v(y)px;
    width: v(y)px;
    min-height: v(z)px;
    min-width: v(w)px;
    z-index: v(q);
}

/* Variable Ranges */
c(0 <= x)
c(0 <= y)
c(0 <= z)
c(x <= 10)
c(y <= 10)
c(z <= 10)

/* 4x + 2y + z >= 24 */
c(4*x+2*y+z >= 24)

/* abs(x - y) <= 2 */
c((x-y)*(x-y) <= 4)

/* min(x, y) = z^2 */
c(w <= x)
c(w <= y)
c((w-y)*(w-x) == 0)
c(w == z*z)

/* Negative */
c(r == -x)
c(q == --x)